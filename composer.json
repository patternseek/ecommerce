{
    "name": "patternseek/ecommerce",
    "type": "library",
    "description": "The Patternseek ECommerce library",
    "keywords": ["library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tolan Blundell",
            "email": "tolan@patternseek.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-geoip": "*",
        "ext-mbstring": "*",
        "patternseek/componentview": "2.*",
        "patternseek/structclass": "1.*",
        "patternseek/dependencyinjector": "0.*",
        "doctrine/annotations": "1.*",
        "doctrine/cache": "1.*",
        "stripe/stripe-php": "6.*",
        "ext-soap": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*.*"
    },
    "autoload": {
        "psr-4": {
            "PatternSeek\\ECommerce\\": "lib",
            "PatternSeek\\ECommerce\\Test\\": "tests"
        }
    }
}
