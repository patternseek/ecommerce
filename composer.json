{
    "name": "patternseek/ecommerce",
    "type": "library",
    "description": "The Patternseek ECommerce library",
    "keywords": ["library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tolan Blundell",
            "email": "tolan@patternseek.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "../patternseek-componentview"
        },
        {
            "type": "vcs",
            "url": "../patternseek-structclass"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-geoip": "*",
        "ext-mbstring": "*",
        "patternseek/componentview": "dev-master",
        "patternseek/structclass": "dev-master",
        "patternseek/dependencyinjector": "dev-master",
        "symfony/validator": "*",
        "doctrine/annotations": "*",
        "doctrine/cache": "*",
        "stripe/stripe-php": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "PatternSeek\\ECommerce\\": "lib",
            "PatternSeek\\ECommerce\\Test\\": "tests"
        }
    }
}
